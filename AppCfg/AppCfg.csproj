<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net462</TargetFrameworks>
    <Title>AppCfg.Net</Title>
    <Version>0.2.1</Version>
    <Copyright>Copyright 2018 minhhungit</Copyright>
    <Description>A mini configuration framework for .NET developers</Description>
    <Authors>Hung Vo</Authors>
    <PackageLicenseUrl>https://github.com/minhhungit/AppCfg/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/minhhungit/AppCfg/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/minhhungit/AppCfg/</RepositoryUrl>
    <PackageTags>configuration appsetting json dotnet csharp netstandard20 dotnetcore</PackageTags>
    <PackageReleaseNotes>add boolean, decimal, datetime parsers</PackageReleaseNotes>
    <AssemblyVersion>0.2.1</AssemblyVersion>
    <FileVersion>0.2.1</FileVersion>
    <Company />
    <PackageIconUrl>https://raw.githubusercontent.com/minhhungit/AppCfg/master/wiki/icon.ico</PackageIconUrl>
    <PackageId>AppCfg.Net</PackageId>
    <Product>AppCfg.Net</Product>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
